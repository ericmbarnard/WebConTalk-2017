    link(rel='stylesheet', href='/css/style.css')
  body
    div.container(ng-app="class-manager")
      block content

  script(type='text/javascript', src="https://code.angularjs.org/1.5.5/angular.min.js")
  script(type='text/javascript', src="js/open-lib.js")
